 
generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
id    String @id @default(cuid())
name String
email String @unique
pass String?
 
image String?
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
 
}